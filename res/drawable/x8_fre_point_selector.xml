<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <stroke android:width="1.0px" android:color="#ff00ffff" />
            <corners android:radius="3.0dip" />
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_selected="false">
        <shape>
            <stroke android:width="1.0px" android:color="@color/white_50" />
            <corners android:radius="3.0dip" />
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1.0px" android:color="@color/white_100" />
            <corners android:radius="3.0dip" />
            <solid android:color="#00000000" />
        </shape>
    </item>
</selector>